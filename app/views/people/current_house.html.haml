%h1= @person.full_name

- unless @house.nil?
  %h3= t('.title')
  %ol.list
    %li.list--item
      %ul.list
        %li= link_to(@house.name, house_path(@house.graph_id))
        %li= "#{l(@house.seat_incumbencies.first.start_date, format: :default)} - " unless @house.seat_incumbencies.first.start_date.nil?

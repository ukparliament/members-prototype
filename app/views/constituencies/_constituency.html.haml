%p
  = "#{l(@constituency.start_date, format: :default)}" unless @constituency.start_date.nil?
  = @constituency.end_date.nil? ? " - Present" : " - #{l(@constituency.end_date, format: :default)}"

%ul.list
  - @seat_incumbencies.each do |seat_incumbency|
    %li.list__item
      = link_to seat_incumbency.member.full_name, person_path(seat_incumbency.member.graph_id)
      %ol
        %li
          = "#{l(seat_incumbency.start_date, format: :default)}" unless seat_incumbency.start_date.nil?
          = seat_incumbency.end_date.nil? ? " - Present" : " - #{l(seat_incumbency.end_date, format: :default)}"

.block
  %nav{ aria:{ label: t('.a_to_z.aria_label') } }
    %ul.navigation__letter
      - ('a'..'z').each do |letter|
        %li{ data: { letter: letter }, class: (params[:letter] == letter) ? 'active' : '' }
          = link_to(send(route_symbol, letter), data: { 'atoz-see': 'true' } ) do
            = letter


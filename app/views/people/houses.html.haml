- page_title = title("#{@person.display_name} - #{t('.title', current_and_former: t('current_and_former').capitalize, house_plural: t('house_plural'))}")

#content.section--primary
  .container
    %h1= page_title

- unless @incumbencies.empty?
  %section
    .container
      %ol.list
        = render :partial => 'people/incumbency', collection: @incumbencies
